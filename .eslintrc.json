{
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "linebreak-style": "off",
        "indent": "off",
        "eol-last": "off",
        "no-use-before-define": "off",
        "space-before-function-paren": "off",
        "func-names": "off",
        "no-console": "off"
    },
    "extends": "airbnb-base/legacy"
}